{
  "name": "shared-async-state-utility",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/async-state/utility/src",
  "prefix": "justinrassier-dot-com",
  "tags": ["scope:shared", "type:utility"],
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/shared/async-state/utility/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/shared/async-state/utility/**/*.ts",
          "libs/shared/async-state/utility/**/*.html"
        ]
      }
    }
  }
}
